{
  "name": "vida",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "prod": "NODE_ENV=production node server",
    "stag": "NODE_ENV=staging node server",
    "build": "APP_ENV=production vite build --ssrManifest --outDir dist/client && APP_ENV=production vite build --ssr src/entry-server.js --outDir dist/server",
    "build:stag": "APP_ENV=staging vite build --mode staging --ssrManifest --outDir dist-staging/client && APP_ENV=staging vite build --mode staging --ssr src/entry-server.js --outDir dist-staging/server",
    "format": "npx prettier --write ./src",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "parse": "node ./i18n-transformer/csv-to-object.js && npx prettier --write ./src/i18n/locale",
    "parse:about": "node ./i18n-transformer/csv-to-json.js && npx prettier --write ./public/about",
    "parse:select": "node ./i18n-transformer/csv-to-json-array.js && npx prettier --write ./public/about/select",
    "pack": "sh pack.sh"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@unhead/vue": "^1.8.1",
    "@vueuse/core": "^10.2.1",
    "@vueuse/integrations": "^10.6.1",
    "axios": "^1.4.0",
    "browser-image-compression": "^2.0.2",
    "compression": "^1.7.4",
    "countries-and-timezones": "^3.5.1",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "hls.js": "^1.4.10",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.4",
    "sha256": "^0.2.0",
    "simple-vue-camera": "^1.1.3",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "vuedraggable": "^4.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.0",
    "@unhead/addons": "^1.8.2",
    "@vitejs/plugin-legacy": "^4.1.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.14",
    "csv-parser": "^3.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "fs": "^0.0.1-security",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.5",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^4.0.0",
    "vue-eslint-parser": "^8.3.0"
  }
}