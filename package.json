{
  "name": "vida",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "ssr": "node server",
    "build": "APP_ENV=production vite build",
    "build:staging": "APP_ENV=staging vite build --mode staging",
    "preview": "vite preview",
    "format": "npx prettier --write ./src",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "parse": "node ./i18n-transformer/csv-to-object.js && npx prettier --write ./src/i18n/locale",
    "parse:about": "node ./i18n-transformer/csv-to-json.js && npx prettier --write ./public/about",
    "parse:select": "node ./i18n-transformer/csv-to-json-array.js && npx prettier --write ./public/about/select",
    "pack": "sh pack.sh"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.2.0",
    "@unhead/vue": "^1.8.3",
    "@vueuse/core": "^10.6.1",
    "@vueuse/integrations": "^10.6.1",
    "axios": "^1.6.1",
    "browser-image-compression": "^2.0.2",
    "countries-and-timezones": "^3.5.2",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "hls.js": "^1.4.12",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "rollup-plugin-terser": "^7.0.2",
    "sha256": "^0.2.0",
    "simple-vue-camera": "^1.1.3",
    "universal-cookie": "^6.1.1",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5",
    "vuedraggable": "^2.24.3",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@unhead/addons": "^1.8.3",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.16",
    "csv-parser": "^3.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "fs": "^0.0.1-security",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^4.0.0",
    "vue-eslint-parser": "^9.3.2"
  }
}