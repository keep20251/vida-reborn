{
  "name": "vida",
  "private": true,
  "version": "1.0.17",
  "type": "module",
  "scripts": {
    "dev": "HMR_SSL=false node server",
    "dev:ssl": "HMR_SSL=true node server",
    "prod": "HMR_SSL=false NODE_ENV=production node server",
    "prod:ssl": "HMR_SSL=true NODE_ENV=production node server",
    "stag": "HMR_SSL=false NODE_ENV=staging node server",
    "stag:ssl": "HMR_SSL=true NODE_ENV=staging node server",
    "build": "APP_ENV=production vite build --ssrManifest --outDir dist/client && APP_ENV=production vite build --ssr src/entry-server.js --outDir dist/server",
    "build:stag": "APP_ENV=staging vite build --mode staging --ssrManifest --outDir dist-staging/client && APP_ENV=staging vite build --mode staging --ssr src/entry-server.js --outDir dist-staging/server",
    "format": "npx prettier --write ./src",
    "lint": "npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "parse": "I18N_ID=1y5SbViL42a3OryBFqRjvYLsemTifDEX7b9YbpVUxx5s node ./i18n-transformer/json-to-ts.js && npx prettier --write ./src/i18n/locale",
    "parse:about": "node ./i18n-transformer/csv-to-json.js && npx prettier --write ./public/about",
    "parse:select": "node ./i18n-transformer/csv-to-json-array.js && npx prettier --write ./public/about/select",
    "publish": "sh publish.sh"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@unhead/ssr": "^1.8.9",
    "@unhead/vue": "^1.8.8",
    "@vueuse/core": "^10.6.1",
    "@vueuse/integrations": "^10.6.1",
    "axios": "^1.6.2",
    "browser-image-compression": "^2.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "countries-and-timezones": "^3.5.2",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "hls.js": "^1.4.12",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "sha256": "^0.2.0",
    "simple-vue-camera": "^1.1.3",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.1",
    "vue": "^3.3.8",
    "vue-i18n": "^9.7.1",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@unhead/addons": "^1.8.8",
    "@vitejs/plugin-legacy": "^4.1.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.16",
    "csv-parser": "^3.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "fs": "^0.0.1-security",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^5.1.0",
    "vue-eslint-parser": "^9.3.2"
  },
  "browserslist": [
    "> 0.25%, not dead"
  ]
}
